<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
	<FileVersion major="1" minor="6" />
	<Project>
		<Option title="gamewatch" />
		<Option pch_mode="2" />
		<Option compiler="devkitgp2x" />
		<Build>
			<Target title="Debug">
				<Option output="bin/GP2XDebug/gamewatch" prefix_auto="1" extension_auto="1" />
				<Option object_output="obj/GP2XDebug/" />
				<Option type="1" />
				<Option compiler="devkitgp2x" />
				<Compiler>
					<Add option="-g" />
				</Compiler>
			</Target>
			<Target title="Release">
				<Option output="bin/GP2XRelease/gamewatch" prefix_auto="1" extension_auto="1" />
				<Option object_output="obj/GP2XRelease/" />
				<Option type="0" />
				<Option compiler="devkitgp2x" />
				<Compiler>
					<Add option="-O2" />
				</Compiler>
				<Linker>
					<Add option="-s" />
				</Linker>
			</Target>
		</Build>
		<Compiler>
			<Add option="-Wall" />
			<Add option="-DGP2X" />
			<Add directory="$(#gp2x_sdl.include)" />
			<Add directory="$(#gp2x_sdl_image.include)" />
			<Add directory="src" />
			<Add directory="$(#gp2x_boost.include)" />
			<Add directory="$(#gp2x_sdl_mixer.include)" />
		</Compiler>
		<Linker>
			<Add option="-static" />
			<Add library="SDL_mixer" />
			<Add library="SDL" />
			<Add library="SDLmain" />
			<Add library="SDL_image" />
			<Add library="boost_filesystem-$(#gp2x_boost.suffix)" />
			<Add library="png" />
			<Add library="jpeg" />
			<Add library="z" />
			<Add library="pthread" />
			<Add library="dl" />
			<Add directory="$(#gp2x_sdl.lib)" />
			<Add directory="$(#gp2x_sdl_image.lib)" />
			<Add directory="$(#gp2x_boost.lib)" />
			<Add directory="$(#gp2x_sdl_mixer.lib)" />
		</Linker>
		<Unit filename="src/device.cpp" />
		<Unit filename="src/device.h" />
		<Unit filename="src/devices/dev_monkey.cpp" />
		<Unit filename="src/devices/dev_monkey.h" />
		<Unit filename="src/gamewatch.cpp" />
		<Unit filename="src/gwdefs.h" />
		<Unit filename="src/plat/plat_desktop.cpp" />
		<Unit filename="src/plat/plat_desktop.h" />
		<Unit filename="src/plat/plat_gp2x.cpp" />
		<Unit filename="src/plat/plat_gp2x.h" />
		<Extensions>
			<code_completion />
			<envvars />
			<lib_finder disable_auto="1" />
			<debugger />
		</Extensions>
	</Project>
</CodeBlocks_project_file>
